Fs = 10000;
n=1:10000;
sin_c1=sin(2*pi*1209*n/Fs);
sin_c2=sin(2*pi*1336*n/Fs);
sin_c3=sin(2*pi*1477*n/Fs);
sin_l1=sin(2*pi*697*n/Fs);
sin_l2=sin(2*pi*770*n/Fs);
sin_l3=sin(2*pi*852*n/Fs);
sin_l4=sin(2*pi*941*n/Fs);
touche1=sin_c1+sin_l1;
touche2=sin_c2+sin_l1;
touche3=sin_c3+sin_l1;
touche4=sin_c1+sin_l2;
touche5=sin_c2+sin_l2;
touche6=sin_c3+sin_l2;
touche7=sin_c1+sin_l3;
touche8=sin_c2+sin_l3;
touche9=sin_c3+sin_l3;
touche0=sin_c2+sin_l4;
